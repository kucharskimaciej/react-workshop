    "cssnano": "^3.10.0",
    "dotenv": "^2.0.0",
    "escape-html": "^1.0.3",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.10.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "html-webpack-pug-plugin": "0.0.3",
    "import-glob": "^1.5.0",
    "import-glob-loader": "^1.1.0",
    "node-sass": "^4.5.0",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
