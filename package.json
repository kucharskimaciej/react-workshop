{
  "scripts": {
    "start": "npm run server:dev",
    "server:dev": "node -r babel-register ./node_modules/.bin/webpack-dev-server --config ./.webpack/development.js --content-base ./app --progress --history-api-fallback",
    "build:dev": "node -r babel-register ./node_modules/.bin/webpack --config ./.webpack/development.js --progress --profile --colors",
    "watch:dev": "node -r babel-register ./node_modules/.bin/webpack --config ./.webpack/development.js --profile --colors --watch",
    "build:production": "node -r babel-register ./node_modules/.bin/webpack --config ./.webpack/production.js",
    "deploy": "gh-pages -d dist"
  },
